<?xml version="1.0"?>
<extension key="ibanrecoder" type="module">
  <file>ibanrecoder</file>
  <name>IBAN Recoder</name>
  <description>Manage IBAN Recoding</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>Patrick Figel</name>
      <email>pfigel@greenpeace.org</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://github.com/greenpeace-cee/ibanrecoder</url>
    <url desc="Documentation">https://github.com/greenpeace-cee/ibanrecoder</url>
    <url desc="Support">https://github.com/greenpeace-cee/ibanrecoder/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-12-16</releaseDate>
  <version>1.0.0-alpha.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.69</ver>
  </compatibility>
  <comments></comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Ibanrecoder</namespace>
    <format>24.09.1</format>
    <angularModule>crmIbanrecoder</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.3</mixin>
    <mixin>entity-types-php@2.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
  <upgrader>CiviMix\Schema\Ibanrecoder\AutomaticUpgrader</upgrader>
</extension>
